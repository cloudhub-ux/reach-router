{
  "name": "@gatsbyjs/reach-router",
  "version": "1.3.10-no-navigate.0",
  "description": "Next generation Routing for React.",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.modern.mjs",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "microbundle -f cjs,modern --jsx React.createElement --generateTypes false",
    "watch": "npm run build watch --no-compress",
    "prepare": "cross-env NODE_ENV=production npm run clean && npm run build",
    "clean": "del-cli dist/*",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "format": "prettier 'src/**/*.js' --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "fix": "npm run format && npm run lint:fix",
    "postinstall": "husky install"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testRegex": ".+\\.test\\.js$",
    "testEnvironmentOptions": {
      "url": "http://localhost"
    }
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "babel-preset-gatsby-package": "^2.20.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.22.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^7.0.0",
    "jest": "^28.1.3",
    "microbundle": "^0.15.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "experimental",
    "react-test-renderer": "experimental"
  },
  "author": "Ryan Florence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/reach-router.git"
  },
  "keywords": [
    "react",
    "react router",
    "gatsby"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "arrowParens": "avoid"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/reach-router/issues"
  },
  "homepage": "https://github.com/gatsbyjs/reach-router#readme"
}
